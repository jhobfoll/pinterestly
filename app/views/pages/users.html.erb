<div class="jumbotron center">
	<h1>Users</h1>

	<div id="Users" class="transitions-enabled">
	<table>
	<tr>
		<th>User</th>
		<th>Created</th>
		<th>Email</th>
	</tr>
	<% @user.each do |user| %>
    <tr>
    	<td><strong><%= user %></strong></td>
      <td><strong><%= user.created_at %></strong></td> 
      <td><strong><%= user.email %></strong></td>
     </tr>	
   <% end %>
 </table>
	</div>
</div>